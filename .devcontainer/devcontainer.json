{
  "name": "A day a song project",
  "dockerComposeFile": [
    "../docker-compose.yml",
    "docker-compose.yml"
  ],
  "service": "adas_api",
  "workspaceFolder": "/home/app",
  "shutdownAction": "stopCompose",
  "extensions": [
	"ms-python.vscode-pylance",
	"visualstudioexptteam.vscodeintellicode",
	"redhat.vscode-yaml",
	"esbenp.prettier-vscode",
	"ryanluker.vscode-coverage-gutters",
	"oderwat.indent-rainbow",
	"mikestead.dotenv",
	"ms-python.python",
	"alanwalk.markdown-toc",
	"darkriszty.markdown-table-prettify",
	"2gua.rainbow-brackets",
	"brainfit.vscode-importmagic",
	"rangav.vscode-thunder-client",
	"davidanson.vscode-markdownlint",
	"mtxr.sqltools",
	"mtxr.sqltools-driver-pg",
	"4ops.terraform",
	"run-at-scale.terraform-doc-snippets",
	"shakram02.bash-beautify",
	"markis.code-coverage",
	"ms-azuretools.vscode-docker",
	"hashicorp.terraform",
	"VisualStudioExptTeam.vscodeintellicode",
	"Swellaby.vscode-rust-test-adapter",
	"rust-lang.rust-analyzer",
	"1YiB.rust-bundle",
	"svelte.svelte-vscode",
	"1YiB.svelte-bundle"
],
  "settings": {
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.profiles.linux": {
      "zsh": {
        "path": "/usr/bin/zsh",
        "args": ["-l"]
      }
    },
    "thunder-client.workspaceRelativePath": ".thunderclient",
    "thunder-client.showActivityFromCollection": true,
    "coverage-gutters.coverageReportFileName": "coverage/index.html",
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "files.eol": "\n",
    "thunder-client.saveToWorkspace": true,
    "thunder-client.certificates": [
    ],   
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "python.formatting.provider": "black",
    "python.testing.pytestArgs": [],    
    "python.linting.banditEnabled": true,
    "python.formatting.blackArgs": ["--line-length=99"],
    "python.testing.pytestEnabled": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.languageServer": "Pylance",
    "python.analysis.autoImportCompletions": true,
    "[python]": {
      "editor.codeActionsOnSave": {
        "source.organizeImports": true
      }
    }
  }
}